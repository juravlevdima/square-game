{
  "name": "express-cra-redux-boilerplate",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "author": "Juravlev Dmitrii",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production yarn run client:build && node server/server.js",
    "server": "nodemon server/server.js",
    "client": "yarn --cwd client run start",
    "client:install": "yarn --cwd client install",
    "client:build": "yarn --cwd client run build",
    "dev": "cross-env NODE_ENV=development concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild": "yarn run client:install"
  },
  "dependencies": {
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.0",
    "postcss": "^8.3.2",
    "postcss-cli": "^8.3.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "tailwindcss": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "ignore": []
  }
}
